<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Resumes</title>
</head>
<body>
    <h1>Resume Listings</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <h3><%= user.name %></h3>
                <p>Email: <%= user.email %></p>
                <p>Resume: <%= user.resume %></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>
